{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "rootDir": ".",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["**/*.ts", "**/*.js", "scripts/manipulate.cjs", "bot.cjs", "hardhat.config.cjs", "helpers/initialization.cjs", "scripts/deploy.cjs", "helpers/server.cjs", "helpers/helpers.cjs", "helpers/abi.cjs"],
  "exclude": ["node_modules"]
}